{
  "type": "module",
  "name": "eLibrary",
  "version": "0.0.1",
  "description": "An Electron application with Vite + React",
  "main": "./out/main/index.js",
  "author": "M&J",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win --config electron-builder.yml",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.yml",
    "build:linux": "npm run build && electron-builder --linux --config electron-builder.yml"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@react-pdf/renderer": "^4.3.2",
    "@recharts/devtools": "^0.0.5",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-table": "^8.21.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.9",
    "chart.js": "^4.4.8",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "electron-devtools-installer": "^4.0.0",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "html2canvas": "^1.4.1",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.562.0",
    "papaparse": "^5.5.2",
    "pdf-lib": "^1.17.1",
    "react-calendar": "^5.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^16.5.1",
    "react-icons": "^5.4.0",
    "react-modal": "^3.16.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.5",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.3",
    "recharts": "^3.6.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "javascript-obfuscator": "^4.2.1",
    "postcss": "^8.5.2",
    "prettier": "3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.0.6",
    "vite": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jason090687/myapp.git"
  },
  "build": {
    "appId": "com.countmein.myapp",
    "productName": "eLibrary",
    "asar": true,
    "files": [
      "out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "icon": "./build/icon.ico",
      "target": [
        "nsis",
        "portable"
      ],
      "sign": null,
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "icon": "./build/icon.ico",
      "target": "nsis"
    }
  }
}
